generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  password  String
  profile   String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}


model Message {
  id        Int      @id @default(autoincrement())
  email     String
  subject   String
  message   String
  createdAt DateTime @default(now())
}